package testapp;

public class SimpleCalc{
  private double a;
  private double b;
    SimpleCalc(double p1, double p2){
      this.a=p1;
      this.b= p2;
    }
    public double getSum(){
      return this.a+this.b;
    }
    public double getSub(){
      return this.a-this.b;
    }
    public double getMno(){
      return this.a*this.b;
    }
    public double getDil(){
      return this.a/this.b;
    }
}

